      <Query>
        <DataSourceName>Proyecto Práctica 06</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT {[Measures].[Sales Amount], [Measures].[Tax Amt]} ON 0,
BOTTOMCOUNT(
        FILTER(
		    FILTER(
				{[Dim Product].[English Product Name].allmembers * [Ship Date].[English Month Name].allmembers * [Dim Sales Territory].[Sales Territory Country].allmembers },
				[Measures].[Sales Amount] &gt; 0),
            NOT IsEmpty([Measures].[Sales Amount])
        ),
        10,
        [Measures].[Sales Amount]
) ON 1
FROM [Adventure Works DW2012]
WHERE [Ship Date].[Calendar Year].&amp;[2012];</CommandText>
